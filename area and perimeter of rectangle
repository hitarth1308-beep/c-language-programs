#include <stdio.h>
#include <conio.h>
int main()
{
    float L,B,area,perimeter;
    printf ("Enter the length of rectangle");
    scanf ("%f", &L);
    printf("enter the breadth of rectanglr");
    scanf ("%f", $B);
    area = L*B;
    perimeter = 2*(L+8);
    printf ("the area of rectangle is %.2f\n",area);
    printf ("the perimeter of rectangle is %.2f\n",perimeter);
    return 0;
}
